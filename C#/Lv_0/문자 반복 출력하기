using System;

public class 문자_반복_출력하기 {
    public string solution(string my_string, int n) {
        string str = "";
        for(int i=0; i<my_string.Length; i++)
            for(int j=0; j<n; j++)
                str = str + my_string[i];
        string answer = str;
        return answer;
    }
}
using System;

public class 숫자_문자열과_영단어 {
    public int solution(string s) {
        string[] str = {"zero","one","two","three","four","five","six","seven","eight","nine"};
        for(int i=0; i<str.Length; i++)
            s = s.Replace(str[i], i.ToString());
        int answer = int.Parse(s);
        return answer;
    }
}
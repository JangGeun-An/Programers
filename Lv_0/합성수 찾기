using System;

public class 합성수_찾기 {
    public int solution(int n) {
        int answer = 0;
        for(int i=1; i<=n; i++)
        {
            int a = 2;
            while(i>a)
            {
                if(i % a == 0)
                {
                    answer++;
                    break;
                }
                else a++;
            }
        }
        return answer;
    }
}
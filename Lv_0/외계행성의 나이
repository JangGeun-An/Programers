using System;

public class 외계행성의_나이 {
    public string solution(int age) {
        string answer = "";
        char[] c = new char[]{'a','b','c','d','e','f','g','h','i','j'};
        while(age>0)
        {
            answer = c[age % 10] + answer;
            age = age / 10;
        }
        return answer;
    }
}
using System;
using System.Collections.Generic;

public class Solution {
    public string[] solution(string my_str, int n) {
        List<string> a = new List<string>();
        int i = 0;
        for(i=0; i<my_str.Length)
        while(true)
        {
            a.Add(my_str.Substring(i, i+n));
            i = i+n+1;
            Console.Write(i);
            //if(i<=my_str.Length && my_str.Length<=i+n) break;
            break;
        }
        string[] answer = a.ToArray();
        return answer;
    }
}